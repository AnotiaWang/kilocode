{
  "compilerOptions": {
    "module": "Node16",
    "target": "ES2022",
    "outDir": "e2e/compiled",
    "sourceMap": true,
    "strict": true,
    "rootDir": "e2e",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    "test-resources",
    "e2e/resources/**",
    ".test-extensions"
  ],
  "include": ["e2e"]
}
